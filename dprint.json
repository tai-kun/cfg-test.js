{
  "excludes": [
    "**/.cache",
    "**/.history",
    "**/dist",
    "**/node_modules",
    "**/*-lock.json"
  ],
  "lineWidth": 80,
  "typescript": {
    "semiColons": "asi",
    "trailingCommas": "onlyMultiLine",
    "singleBodyPosition": "nextLine",
    "arrowFunction.useParentheses": "preferNone"
  },
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.90.1.wasm",
    "https://plugins.dprint.dev/json-0.19.2.wasm"
  ]
}
