{
  "name": "cfg-test",
  "version": "0.0.0",
  "description": "In-source testing using Node.js Test Runner",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "src",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "default": "./src/auto.cjs"
    },
    "./api": {
      "default": "./src/api.cjs"
    },
    "./define": {
      "types": "./src/define.d.ts",
      "default": "./src/define.cjs"
    },
    "./manual": {
      "default": "./src/manual.cjs"
    },
    "./globals": {
      "types": "./globals.d.ts"
    }
  },
  "optionalDependencies": {
    "@swc-node/register": ">=1.6.5",
    "ts-node": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tai-kun/cfg-test.js"
  },
  "author": {
    "name": "tai-kun",
    "url": "https://github.com/tai-kun"
  },
  "scripts": {
    "ci": "bash scripts/ci.sh",
    "test": "bash scripts/test.sh",
    "update": "bash scripts/update.sh"
  },
  "volta": {
    "node": "18.17.1"
  },
  "keywords": [
    "cfg",
    "cfg-test",
    "test",
    "test-runner",
    "typescript"
  ]
}
